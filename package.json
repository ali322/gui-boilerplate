{
  "name": "gui-boilerplate",
  "version": "0.0.1",
  "description": "gui boilerplate",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "node script/dev.runner.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config script/webpack.prod.js",
    "dev": "webpack-dev-server --hot --inline --port 8080 --config script/webpack.dev.js",
    "pack:osx": "cross-env BUILD_TARGET=darwin node script/pack.js",
    "pack:win": "cross-env BUILD_TARGET=win32 node script/pack.js",
    "pack": "npm run pack:osx && npm run pack:win",
    "test": "eslint app"
  },
  "author": "alichen",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "electron": "^1.6.11",
    "electron-debug": "^2.0.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^12.2.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^4.7.1",
    "inject-html-webpack-plugin": "0.0.39",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.4.5",
    "resolve-url-loader": "^3.0.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "tidy-stats-webpack-plugin": "0.0.7",
    "vue": "^2.5.17",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.1.4"
  }
}
