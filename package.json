{
  "name": "gui-boilerplate",
  "version": "0.0.1",
  "description": "gui boilerplate",
  "main": "dist/main/index.js",
  "scripts": {
    "clean": "rimraf dist/renderer",
    "start": "node script/dev-runner.js",
    "dll": "rimraf dist/renderer/vendor && cross-env NODE_ENV=production webpack --config script/webpack.dll.js",
    "build:renderer": "cross-env NODE_ENV=production webpack --config script/webpack.prod.js",
    "build:main": "cross-env NODE_ENV=production webpack --config script/webpack.main.js",
    "build": "npm run build:main && npm run build:renderer",
    "dev": "webpack-dev-server --hot --inline --port 8080 --config script/webpack.dev.js",
    "pack:osx": "cross-env BUILD_TARGET=darwin node script/builder.js",
    "pack:win": "cross-env BUILD_TARGET=win32 node script/builder.js",
    "pack": "npm run pack:osx && npm run pack:win",
    "sign": "electron-osx-sign output/aid-client-darwin-x64/aid-client.app",
    "lint": "eslint renderer"
  },
  "author": "alichen",
  "license": "MIT",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "@hot-loader/react-dom": "^16.8.6",
    "@types/axios": "^0.14.0",
    "@types/form-data": "^2.2.1",
    "@types/fs-extra": "^5.0.5",
    "@types/lodash": "^4.14.122",
    "@types/prop-types": "^15.7.1",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "@types/semver": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "awesome-typescript-loader": "^5.2.1",
    "chunk-asset-webpack-plugin": "0.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "date-fns": "^1.30.1",
    "download-git-repo": "^1.1.0",
    "electron": "^1.8.3",
    "electron-builder": "^20.39.0",
    "electron-debug": "^2.0.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-log": "^3.0.1",
    "electron-updater": "^3.0.4",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^3.0.1",
    "form-data": "^2.3.3",
    "globby": "^9.1.0",
    "inject-html-webpack-plugin": "0.0.39",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mobx": "^5.10.0",
    "mobx-react": "^6.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.9.0",
    "react-router-dom": "^5.0.1",
    "resolve-url-loader": "^3.0.1",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "tidy-stats-webpack-plugin": "0.0.12",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3333",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.2.1"
  }
}
